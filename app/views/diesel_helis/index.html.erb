<p id="notice"><%= notice %></p>

<h1>Listing Diesel Helis</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @all_helis.each do |all_helis| %>
      <tr>
        <td><%= all_helis.ten %></td>
        <td><%= link_to 'Show', all_helis %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @all_helis,renderer: BootstrapPagination::Rails %>

<br>
