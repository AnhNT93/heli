<p id="notice"><%= notice %></p>

<h1>Listing Pallets</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Ten</th>
      <th>Gioithieu</th>
      <th>Nhasanxuat</th>
      <th>Model</th>
      <th>Trongtai</th>
      <th>Trongtamtai</th>
      <th>Tocdonang</th>
      <th>Chieucaonangtieuchuan</th>
      <th>Tocdodichuyen</th>
      <th>Bankinhquayvongtoithieu</th>
      <th>Khanangleodoc</th>
      <th>Loaidieukhienlai</th>
      <th>Motolai</th>
      <th>Motobom</th>
      <th>Acquy</th>
      <th>Kichthuoccangnang</th>
      <th>Colop</th>
      <th>Tongkhoiluong</th>
      <th>Tongchieudai</th>
      <th>Tongchieurong</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pallets.each do |pallet| %>
      <tr>
        <td><%= pallet.type_id %></td>
        <td><%= pallet.ten %></td>
        <td><%= pallet.gioiThieu %></td>
        <td><%= pallet.nhaSanXuat %></td>
        <td><%= pallet.model %></td>
        <td><%= pallet.trongTai %></td>
        <td><%= pallet.trongTamTai %></td>
        <td><%= pallet.tocDoNang %></td>
        <td><%= pallet.chieuCaoNangTieuChuan %></td>
        <td><%= pallet.tocDoDiChuyen %></td>
        <td><%= pallet.banKinhQuayVongToiThieu %></td>
        <td><%= pallet.khaNangLeoDoc %></td>
        <td><%= pallet.loaiDieuKhienLai %></td>
        <td><%= pallet.motoLai %></td>
        <td><%= pallet.motoBom %></td>
        <td><%= pallet.acQuy %></td>
        <td><%= pallet.kichThuocCangNang %></td>
        <td><%= pallet.coLop %></td>
        <td><%= pallet.tongKhoiLuong %></td>
        <td><%= pallet.tongChieuDai %></td>
        <td><%= pallet.tongChieuRong %></td>
        <td><%= link_to 'Show', pallet %></td>
        <td><%= link_to 'Edit', edit_pallet_path(pallet) %></td>
        <td><%= link_to 'Destroy', pallet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

