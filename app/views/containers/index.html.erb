<p id="notice"><%= notice %></p>

<h1>Listing Containers</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Ten</th>
      <th>Gioithieu</th>
      <th>Nhasanxuat</th>
      <th>Model</th>
      <th>Taitrongnang</th>
      <th>Trongtamtai</th>
      <th>Sohang</th>
      <th>Tocdonang</th>
      <th>Tocdodichuyen</th>
      <th>Bankinhquayvongtoithieu</th>
      <th>Khanangleodoc</th>
      <th>Luckeo</th>
      <th>Chieucaocanang</th>
      <th>Chieuconcannangkhinanghetco</th>
      <th>Lopxe</th>
      <th>Dongco</th>
      <th>Congsuatlstring</th>
      <th>Momen</th>
      <th>Tongchieudai</th>
      <th>Tongchieurong</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @containers.each do |container| %>
      <tr>
        <td><%= container.type_id %></td>
        <td><%= container.ten %></td>
        <td><%= container.gioiThieu %></td>
        <td><%= container.nhaSanXuat %></td>
        <td><%= container.model %></td>
        <td><%= container.taiTrongNang %></td>
        <td><%= container.trongTamTai %></td>
        <td><%= container.soHang %></td>
        <td><%= container.tocDoNang %></td>
        <td><%= container.tocDoDiChuyen %></td>
        <td><%= container.banKinhQuayVongToiThieu %></td>
        <td><%= container.khaNangLeoDoc %></td>
        <td><%= container.lucKeo %></td>
        <td><%= container.chieuCaoCanang %></td>
        <td><%= container.chieuConCanNangKhiNangHetCo %></td>
        <td><%= container.lopXe %></td>
        <td><%= container.dongCo %></td>
        <td><%= container.congSuatLstring %></td>
        <td><%= container.momen %></td>
        <td><%= container.tongChieuDai %></td>
        <td><%= container.tongChieuRong %></td>
        <td><%= link_to 'Show', container %></td>
        <td><%= link_to 'Edit', edit_container_path(container) %></td>
        <td><%= link_to 'Destroy', container, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

