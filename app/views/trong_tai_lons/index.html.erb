        <td><%= trong_tai_lon.khoangDichChuyenCangNang %></td>
        <td><%= trong_tai_lon.coLopXe %></td>
        <td><%= trong_tai_lon.loaiDongCo %></td>
        <td><%= trong_tai_lon.congSuat %></td>
        <td><%= trong_tai_lon.momen %></td>
        <td><%= trong_tai_lon.chieuDaiXe %></td>
        <td><%= trong_tai_lon.chieuRong %></td>
        <td><%= link_to 'Show', trong_tai_lon %></td>
        <td><%= link_to 'Edit', edit_trong_tai_lon_path(trong_tai_lon) %></td>
        <td><%= link_to 'Destroy', trong_tai_lon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

