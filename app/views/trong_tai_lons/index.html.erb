      <th>Chieucaokhungnangkhinangcaonhat</th>
      <th>Kichthuoccangnang</th>
      <th>Khoangdichchuyencangnang</th>
      <th>Colopxe</th>
      <th>Loaidongco</th>
      <th>Congsuat</th>
      <th>Momen</th>
      <th>Chieudaixe</th>
      <th>Chieurong</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trong_tai_lons.each do |trong_tai_lon| %>
      <tr>
        <td><%= trong_tai_lon.type_id %></td>
        <td><%= trong_tai_lon.ten %></td>
        <td><%= trong_tai_lon.gioiThieu %></td>
        <td><%= trong_tai_lon.nhaSanXuat %></td>
        <td><%= trong_tai_lon.model %></td>
        <td><%= trong_tai_lon.taitrongNang %></td>
        <td><%= trong_tai_lon.trongTamTai %></td>
        <td><%= trong_tai_lon.tocDoNang %></td>
        <td><%= trong_tai_lon.chieuCaoNangLonNhat %></td>
        <td><%= trong_tai_lon.tocDoDiChuyen %></td>
        <td><%= trong_tai_lon.banKinhQuayVongToiThieu %></td>
        <td><%= trong_tai_lon.khaNangLeoDOc %></td>
        <td><%= trong_tai_lon.sucKeo %></td>
